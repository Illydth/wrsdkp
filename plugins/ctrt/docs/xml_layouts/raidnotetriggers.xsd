<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:element name="RaidNoteTriggers">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="RaidNoteTrigger" type="RaidNoteTriggerType" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="RaidNoteTriggerType">
		<xs:sequence>
			<xs:element ref="trigger"/>
			<xs:element ref="result"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="trigger" type="xs:string"/>
	<xs:element name="result" type="xs:string"/>
</xs:schema>
